  <SerivceAddress />
  <Port>4322</Port>
  <EnableIPv6>false</EnableIPv6>

  <DefaultLanguage />

  <Diagnostic>
    <EnableTraceLogging>false</EnableTraceLogging>
  </Diagnostic>
</Services>