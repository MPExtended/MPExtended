  <AccessRequestEnabled>true</AccessRequestEnabled>

  <NetworkImpersonation>
    <Domain />
    <Username />
    <EncryptedPassword />
    <ReadInStreamingService>true</ReadInStreamingService>
  </NetworkImpersonation>
  
  <BonjourEnabled>true</BonjourEnabled>
  <BonjourName />

  <ExternalAddress>
    <Autodetect>true</Autodetect>
    <Custom />
