@using MPExtended.Services.TVAccessService.Interfaces
@using MPExtended.Services.StreamingService.Interfaces
@model WebRecordingBasic
@{
    ViewBag.Title = "Recording";
}

<h2>@Model.Title</h2>
<ul>
<li>Recorded on: @Model.StartTime.ToShortDateString()</li>
<li>@Html.ActionLink("Watch", "WatchRecording", new { id = Model.Id })</li>
<li>@Html.ActionLink("Download", "Download", "Stream", new { item = Model.Id, type = WebStreamMediaType.Recording }, new { })</li>
</ul>