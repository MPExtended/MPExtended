@model IEnumerable<MPExtended.Applications.WebMediaPortal.Models.SearchResultsViewModel>
@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

@if (Model.Count() == 0)
{
    <em>Sorry, your search didn't yield any results.</em>
}

<ul>
@foreach (var result in Model)
{
    <li><a href="@result.URL">@result.Title</a></li>
}
</ul>
